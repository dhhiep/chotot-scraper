$ ->
  $('#logs-table').dataTable
    responsive: true
    processing: true
    autoWidth: false
    pageLength: 25
    oLanguage:
      sProcessing: '<%= image_tag("Spinner.gif") %>'
    serverSide: true
    searchDelay: 800
    ajax: $('#logs-table').data('source')
    pagingType: 'full_numbers'
    columns: [
      {data: 'uuid'}
      {data: 'description'}
    ]
